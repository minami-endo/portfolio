<h1>Admins::Items#index</h1>
<p>Find me in app/views/admins/items/index.html.erb</p>


<h1>商品</h1>

<% @items.each do |item| %>
    <%= attachment_image_tag item, :image, size:'150x100' %>
	<%= link_to item.name, admins_item_path(item) %>
	<%= item.caption %>
	<%= item.price %>
<% end %>
<%= paginate @items %>
