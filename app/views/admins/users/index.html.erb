<h1>Admins::Users#index</h1>
<p>Find me in app/views/admins/users/index.html.erb</p>


<h1>ユーザー一覧</h1>
<div class="container">
	
		<% @users.each do |user| %>
		<div class="index_user_box">
		    <%= attachment_image_tag user, :image, size:'150x100' %>
			<%= user.name %>
			<%= user.comment %>
			<%= user.email %>
		    <%= user.phone_number %>
		    <%= user.postal_code %>
		    <%= user.address %>
		    <%= user.is_deleted_i18n %>
		    <%= link_to "このユーザー情報を見る", admins_user_path(user) %>
		    </div>
		<% end %>
	
<%= paginate @users %>

</div>